{
  "name": "rss",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-rss",
    "stop": "egg-scripts stop --title=egg-server-rss",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean",
    "init": "yarn install && cd web && yarn install",
    "build": "cd web && cross-env NODE_ENV=production webpack --progress --config webpack.prod.js",
    "stats": "cd web && cross-env NODE_ENV=production webpack --progress --config webpack.prod.js --json > stats.json",
    "dll": "cd web && cross-env NODE_ENV=production webpack --config webpack.dll.js",
    "devweb": "cd web && cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.js --open",
    "eslint": "cd web && eslint --ext .js --ext .jsx src/",
    "eslint-fix": "cd web && eslint --fix --ext .js --ext src/",
    "csslint": "cd web && stylelint src/**/*.scss",
    "css-fix": "cd web && stylelint src/**/*.scss --fix"
  },
  "dependencies": {
    "egg": "^2.6.1",
    "egg-scripts": "^2.6.0",
    "ejs": "^3.0.2",
    "epub": "^1.2.0",
    "lowdb": "^1.0.0",
    "moment": "^2.24.0",
    "node-cmd": "^3.0.0",
    "pm2": "^4.2.3",
    "rss-parser": "^3.7.6",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "mmojianpeng",
  "license": "MIT"
}
